   "source": [
    "type RandomState a = State StdGen a\n",
    "\n",
    "getRandom :: Random a => RandomState a\n",
    "getRandom =\n",
    "    get >>= \\g ->\n",
    "    let (v, g') = random g in\n",
    "    put g' >>\n",
    "    return v"
